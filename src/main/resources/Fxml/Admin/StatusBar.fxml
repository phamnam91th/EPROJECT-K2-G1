    <HBox alignment="CENTER" layoutX="1152.0" prefHeight="50.0" prefWidth="154.0" spacing="20.0">
        <Button fx:id="minimize_btn" mnemonicParsing="false" prefHeight="40.0" prefWidth="50.0">
            <graphic>
                <FontAwesomeIcon glyphName="MINUS" size="18" />
            </graphic>
        </Button>
        <Button fx:id="close_btn" mnemonicParsing="false" prefHeight="40.0" prefWidth="50.0">
            <graphic>
                <FontAwesomeIcon glyphName="CLOSE" size="18" />
            </graphic>
        </Button>
    </HBox>
</AnchorPane>
